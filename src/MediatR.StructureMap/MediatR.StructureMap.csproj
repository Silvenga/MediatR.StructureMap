<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net461;netstandard2.0</TargetFrameworks>
    <Version>0.0.0.1</Version>
    <Authors>Silvenga</Authors>
    <PackageLicenseUrl>https://github.com/Silvenga/MediatR.StructureMap/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/Silvenga/MediatR.StructureMap</PackageProjectUrl>
    <Description>StructureMap configuration helper for MediatR.</Description>
    <PackageTags>StructureMap;MediatR</PackageTags>
    <RestorePackagesWithLockFile>true</RestorePackagesWithLockFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="MediatR" Version="[6,8)" />
    <PackageReference Include="StructureMap" Version="4.4.3" />
  </ItemGroup>

</Project>